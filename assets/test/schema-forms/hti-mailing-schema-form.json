{
  "schema": {
    "type": "object",
    "properties": {
      "addressLine1": {
        "notitle": true,
        "fieldHtmlClass": "error-red",
        "placeholder": "Address Line 1*",
        "type": "string",
        "minLength": 3,
        "maxLength": 40,
        "pattern": "^[a-zA-Z0-9~`\\!@#\\$%\\^&*\\(\\)_\\+\\-\\=\\{\\}\\|\"\\:<>\\?,/\\\\.\\[\\];'\\s]*$",
        "validationMessages": {
          "required": "Required",
          "pattern": "Please enter a valid Address.",
          "minLength": "Minimum length should be 3 characters.",
          "maxLength": "Maximum length should be 40 characters."
        },
        "feedbackOnRender": true,
        "customErrorMessage": ""
      },
      "addressLine2": {
        "notitle": true,
        "fieldHtmlClass": "error-red not-required",
        "placeholder": "Address Line 2",
        "type": "string",
        "minLength": 3,
        "maxLength": 40,
        "pattern": "^[a-zA-Z0-9~`\\!@#\\$%\\^&*\\(\\)_\\+\\-\\=\\{\\}\\|\"\\:<>\\?,/\\\\.\\[\\];'\\s]*$",
        "validationMessages": {
          "required": "Required",
          "pattern": "Please enter a valid Address.",
          "minLength": "Minimum length should be 3 characters.",
          "maxLength": "Maximum length should be 40 characters."
        },
        "feedbackOnRender": true,
        "customErrorMessage": ""
      },
      "city": {
        "notitle": true,
        "fieldHtmlClass": "error-red",
        "placeholder": "City*",
        "type": "string",
        "minLength": 3,
        "maxLength": 40,
        "pattern": "^[a-zA-Z0-9~`\\!@#\\$%\\^&*\\(\\)_\\+\\-\\=\\{\\}\\|\"\\:<>\\?,/\\\\.\\[\\];'\\s]*$",
        "validationMessages": {
          "required": "Required",
          "pattern": "Please enter a valid City.",
          "minLength": "Minimum length should be 3 characters.",
          "maxLength": "Maximum length should be 40 characters."
        },
        "feedbackOnRender": true,
        "customErrorMessage": ""
      },
      "state": {
        "notitle": true,
        "type": "string",
        "fieldHtmlClass": "error-red",
        "widget": "custom-select-widget",
        "countryIdRequired": true,
        "selectText": "State",
        "nameField": "stateName",
        "valueField": "stateCode",
        "optionGetUrl": "/admin/V1/states?countryID=98",
        "validationMessages": {
          "required": "Required"
        },
        "feedbackOnRender": true,
        "customErrorMessage": ""
      },
      "zip": {
        "notitle": true,
        "fieldHtmlClass": "error-red",
        "placeholder": "Zip Code*",
        "type": "string",
        "pattern": "^\\d{4,4}$",
        "validationMessages": {
          "required": "Required",
          "pattern": "Please enter a valid Zip Code."
        },
        "feedbackOnRender": true,
        "customErrorMessage": ""
      },
      "country": {
        "notitle": true,
        "type": "string",
        "widget": "custom-select-widget",
        "selectText": "Country",
        "optionGetUrl": "/admin/V1/countries/lookup",
        "nameField": "name",
        "valueField": "countryId",
        "disabled": true,
        "default": 98,
        "filterField": "countryId",
        "customErrorMessage": ""
      }
    },
    "required": [
      "addressLine1",
      "city",
      "zip"
    ]
  },
  "form": [
