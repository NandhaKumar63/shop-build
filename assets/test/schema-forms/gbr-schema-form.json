{
    "schema": {
        "type": "object",
        "properties": {
            "firstName": {
                "notitle": true,
                "placeholder": "First Name*",
                "type": "string",
                "minLength": 1,
                "maxLength": 20,
                "fieldHtmlClass": "error-red",
                "pattern": "^[a-zA-Z0-9 '-]+$",
                "validationMessages": {
                    "required": "Required",
                    "pattern": "Please enter valid name",
                    "minLength": "Minimum length should be 1 characters.",
                    "maxLength": "Maximum length should be 20 characters."
                },
                "customValidationMessage": "",
                "feedbackOnRender": true
            },
            "middleName": {
                "notitle": true,
                "placeholder": "Middle Name",
                "type": "string",
                "minLength": 1,
                "maxLength": 20,
                "fieldHtmlClass": "error-red",
                "pattern": "^[a-zA-Z0-9 '-]+$",
                "validationMessages": {
                    "required": "Required",
                    "pattern": "Please enter valid name",
                    "minLength": "Minimum length should be 1 characters.",
                    "maxLength": "Maximum length should be 20 characters."
                },
                "customValidationMessage": "",
                "feedbackOnRender": true
            },
            "lastName": {
                "notitle": true,
                "placeholder": "Last Name*",
                "type": "string",
                "minLength": 1,
                "maxLength": 20,
                "fieldHtmlClass": "error-red",
                "pattern": "^[a-zA-Z0-9 '-]+$",
                "validationMessages": {
                    "required": "Required",
                    "pattern": "Please enter valid name",
                    "minLength": "Minimum length should be 1 characters.",
                    "maxLength": "Maximum length should be 20 characters."
                },
                "customValidationMessage": "",
                "feedbackOnRender": true
            },
            "addressLine1": {
                "notitle": true,
                "fieldHtmlClass": "error-red",
                "placeholder": "Address Line 1*",
                "type": "string",
                "minLength": 3,
                "maxLength": 50,
                "pattern": "^[a-zA-Z0-9~`\\!@#\\$%\\^&*\\(\\)_\\+\\-\\=\\{\\}\\|\"\\:<>\\?,/\\\\.\\[\\];'\\s]*$",
                "validationMessages": {
                    "required": "Required",
                    "pattern": "Please enter valid address",
                    "minLength": "Minimum length should be 3 characters.",
                    "maxLength": "Maximum length should be 50 characters."
                },
                "feedbackOnRender": true,
                "customErrorMessage": "",
                "hasFloat": true
            },
            "addressLine2": {
                "notitle": true,
                "fieldHtmlClass": "error-red not-required",
                "placeholder": "Address Line 2",
                "type": "string",
                "minLength": 3,
                "maxLength": 50,
                "pattern": "^[a-zA-Z0-9~`\\!@#\\$%\\^&*\\(\\)_\\+\\-\\=\\{\\}\\|\"\\:<>\\?,/\\\\.\\[\\];'\\s]*$",
                "validationMessages": {
                    "required": "Required",
                    "pattern": "Please enter valid address",
                    "minLength": "Minimum length should be 3 characters.",
                    "maxLength": "Maximum length should be 50 characters."
                },
                "feedbackOnRender": true,
                "customErrorMessage": "",
                "hasFloat": true
            },
            "addressLine3": {
                "notitle": true,
                "fieldHtmlClass": "error-red not-required",
                "placeholder": "Address Line 3",
                "type": "string",
                "minLength": 3,
                "maxLength": 50,
                "pattern": "^[a-zA-Z0-9~`\\!@#\\$%\\^&*\\(\\)_\\+\\-\\=\\{\\}\\|\"\\:<>\\?,/\\\\.\\[\\];'\\s]*$",
                "validationMessages": {
                    "required": "Required",
                    "pattern": "Please enter valid address",
                    "minLength": "Minimum length should be 3 characters.",
                    "maxLength": "Maximum length should be 100 characters."
                },
                "feedbackOnRender": true,
                "customErrorMessage": "",
                "hasFloat": true
            },
            "city": {
                "notitle": true,
                "fieldHtmlClass": "error-red",
                "placeholder": "City*",
                "type": "string",
                "minLength": 3,
                "maxLength": 30,
                "pattern": "^[a-zA-Z0-9~`\\!@#\\$%\\^&*\\(\\)_\\+\\-\\=\\{\\}\\|\"\\:<>\\?,/\\\\.\\[\\];'\\s]*$",
                "validationMessages": {
                    "required": "Required",
                    "pattern": "Please enter valid city",
                    "minLength": "Minimum length should be 3 characters.",
                    "maxLength": "Maximum length should be 30 characters."
                },
                "feedbackOnRender": true,
                "customErrorMessage": ""
            },
            "zip": {
                "notitle": true,
                "fieldHtmlClass": "error-red",
                "placeholder": "Zip Code*",
                "type": "string",
                "pattern": "^(GIR|[a-zA-Z]\\d[a-zA-Z\\d]??|[a-zA-Z]{2}\\d[a-zA-Z\\d]??)[ ]??(\\d[a-zA-Z]{2})$",
                "validationMessages": {
                    "required": "Required",
                    "pattern": "Please enter valid zip code.",
                    "maxLength": "Maximum length should be 10 characters.",
                    "postalCodeValidation": "Post Code and Country Combination as mismatch"
                },
                "postalCodeValidation":{
                    "allowedPatterns":[
                        {"countryID": 114, "format": "JE"},
                        {"countryID": 94, "format": "GY"},
                        {"countryID": 109, "format": "IM"}],
                    "controlToCheck":"country"
                  },
                "feedbackOnRender": true,
                "customErrorMessage": "",
                "postRenderValidation": true
            },
            "country": {
                "notitle": true,
                "type": "string",
                "widget": "custom-select-widget",
                "selectText": "Country",
                "optionGetUrl": "/admin/V1/countries/gbr/participating-countries",
                "nameField": "name",
                "valueField": "countryId",
                "disabled": true,
                "default": 235,
                "filterField": "countryId",
                "customErrorMessage": ""
            },
            "phone": {
                "notitle": true,
                "widget": "custom-mask-widget",
                "maskFormat": "00000 000000",
                "fieldHtmlClass": "error-red",
                "placeholder": "Phone Number",
                "type": "string",
                "widgetType":"tel",
                "minLength": 11,
                "maxLength": 12,
                "validationMessages": {
                    "pattern": "Please enter valid phone number",
                    "minLength": "Minimum length should be 10 characters.",
                    "maxLength": "Maximum length should be 15 characters."
                },
                "feedbackOnRender": true
            },
            "phoneType": {
                "notitle": true,
                "type": "number",
                "fieldHtmlClass": "error-red",
                "widget": "custom-select-widget",
                "selectText": "Type",
                "feedbackOnRender": true,
                "customErrorMessage": ""
            },
            "deliveryInstruction": {
                "notitle": true,
                "fieldHtmlClass": "error-red not-required",
                "placeholder": "Enter Delivery Instruction",
                "type": "string",
                "minLength": 0,
                "maxLength": 256,
                "pattern": "^[a-zA-Z0-9~`\\!@#\\$%\\^&*\\(\\)_\\+\\-\\=\\{\\}\\|\"\\:<>\\?,/\\\\.\\[\\];'\\s]*$",
                "validationMessages": {
                    "required": "Required",
                    "pattern": "Please enter valid address",
                    "minLength": "Minimum length should be 3 characters.",
                    "maxLength": "Maximum length should be 50 characters."
                },
                "feedbackOnRender": true,
                "customErrorMessage": "",
                "hasFloat": false
            }
        },
        "required": [
            "firstName",
            "lastName",
            "addressLine1",
            "city",
            "zip"
        ]
    },
